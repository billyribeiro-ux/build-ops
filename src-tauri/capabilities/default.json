{
  "$schema": "https://tauri.app/schema/capability.json",
  "identifier": "default",
  "description": "Default permissions for BuildOps 40",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-exists",
    "fs:allow-create",
    "fs:allow-remove",
    "notification:allow-is-permission-granted",
    "notification:allow-request-permission",
    "notification:allow-notify",
    "os:allow-platform",
    "os:allow-version",
    "clipboard-manager:allow-read",
    "clipboard-manager:allow-write"
  ]
}
